<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<fx:root xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" height="600.0" minHeight="600.0" minWidth="800.0" onCloseRequest="#handleExit" resizable="false" title="Address App" type="javafx.stage.Stage" width="800.0">
    <icons>
        <Image url="@/images/fund_book_32.png" />
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@IchiTheme.css" />
            </stylesheets>
            <VBox prefHeight="600.0" prefWidth="800.0">
                <MenuBar fx:id="menuBar" VBox.vgrow="NEVER">
                    <Menu mnemonicParsing="false" text="File">
                        <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
                    </Menu>
                    <Menu mnemonicParsing="false" text="Help">
                        <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
                    </Menu>
                </MenuBar>
                <VBox styleClass="padded-container">
                    <StackPane fx:id="commandBoxPlaceholder" VBox.vgrow="NEVER" />
                    <StackPane fx:id="resultDisplayPlaceholder" maxHeight="100" minHeight="100" prefHeight="100" />
                    <TabPane side="LEFT" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
                        <tabs>
                            <Tab text="Transaction">
                                <content>
                                    <AnchorPane minHeight="0.0" minWidth="0.0">
                                        <children>
                                            <VBox fx:id="personList" styleClass="pane-with-border" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                <StackPane fx:id="personListPanelPlaceholder" VBox.vgrow="ALWAYS" />
                                            </VBox>
                                        </children>
                                    </AnchorPane>
                                </content>
                            </Tab>
                            <Tab text="Repeaters">
                                <content>
                                    <AnchorPane minHeight="0.0" minWidth="0.0">
                                        <children>
                                            <VBox fx:id="repeaterList" styleClass="pane-with-border" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                <StackPane fx:id="repeaterListPanelPlaceholder" VBox.vgrow="ALWAYS" />
                                            </VBox>
                                        </children>
                                    </AnchorPane>
                                </content>
                            </Tab>
                            <Tab text="Budgets">
                                <content>
                                    <AnchorPane minHeight="0.0" minWidth="0.0">
                                        <children>
                                            <VBox fx:id="budgetList" styleClass="pane-with-border" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                <StackPane fx:id="budgetListPanelPlaceholder" VBox.vgrow="ALWAYS" />
                                            </VBox>
                                        </children>
                                    </AnchorPane>
                                </content>
                            </Tab>
                            <Tab text="Loan">
                                <content>
                                    <AnchorPane minHeight="0.0" minWidth="0.0">
                                        <children>
                                            <VBox fx:id="loanList" styleClass="pane-with-border" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                <StackPane fx:id="loanListPanelPlaceholder" VBox.vgrow="ALWAYS" />
                                            </VBox>
                                        </children>
                                    </AnchorPane>
                                </content>
                            </Tab>
                            <Tab text="Analytics">
                                <content>
                                    <AnchorPane minHeight="0.0" minWidth="0.0">
                                        <children>
                                            <VBox fx:id="analyticsBoard" styleClass="pane-with-border" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                <StackPane fx:id="analyticsBoardPanelPlaceholder" VBox.vgrow="ALWAYS" />
                                            </VBox>
                                        </children>
                                    </AnchorPane>
                                </content>
                            </Tab>
                        </tabs>
                    </TabPane>
                </VBox>
                <StackPane fx:id="statusbarPlaceholder" />
            </VBox>
        </Scene>
    </scene>
</fx:root>
